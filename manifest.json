{
  "manifest_version": 3,
  "name": "unBind",
  "description": "This extension makes browsing more productive",
  "version": "1.0",
  "permissions": ["activeTab", "webNavigation"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["store.js","content.js", "test.js"],
      "css": ["content.css"]
    }
  ]
}
